<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <ImageView
        android:id="@+id/division_Fail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/division" />

    <TextView
        android:id="@+id/failText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="아무래도 우리는 말이 안 통해!"
        android:fontFamily="@font/gowunbatang_bold"
        android:textSize="55px"
        android:background="#80000000"
        android:textColor="@color/white"
        android:padding="20dp"
        android:layout_marginTop="150dp"
        android:layout_marginLeft="45dp"/>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="120dp"
        android:layout_height="250dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="210dp"
        app:srcCompat="@drawable/north_charac_angry" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="120dp"
        android:layout_height="250dp"
        android:layout_marginLeft="190dp"
        android:layout_marginTop="210dp"
        app:srcCompat="@drawable/south_charac_sad" />




</FrameLayout>